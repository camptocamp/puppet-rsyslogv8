{
  "name": "camptocamp-rsyslogv8",
  "version": "1.3.1",
  "author": "Camptocamp",
  "summary": "Puppet Module to install and configure Rsyslog-v8",
  "license": "Camptocamp SA",
  "source": "https://github.com/camptocamp/puppet-rsyslogv8",
  "issues_url": "https://github.com/camptocamp/puppet-rsyslogv8/issues",
